    "commands": {
        "arrow_left": {
            "label": "< Back",
            "cmd": {
                "OSX": "osascript -e 'tell application \"System Events\" to key code 123'"
            }
        },
        "arrow_right": {
            "label": "Forward >",
            "cmd": {
                "OSX": "osascript -e 'tell application \"System Events\" to key code 124'"
            }
        }
    }
}
